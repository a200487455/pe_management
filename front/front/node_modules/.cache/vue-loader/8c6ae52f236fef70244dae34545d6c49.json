r';\r\n\t\t\t\t\tthis.$http.post(url, this.registerForm).then(res => {\r\n\t\t\t\t\t\tif (res.data.code === 0) {\r\n\t\t\t\t\t\t\tthis.$message({\r\n\t\t\t\t\t\t\t\tmessage: '注册成功',\r\n\t\t\t\t\t\t\t\ttype: 'success',\r\n\t\t\t\t\t\t\t\tduration: 1500,\r\n\t\t\t\t\t\t\t\tonClose: () => this.$router.push('/login')\r\n\t\t\t\t\t\t\t});\r\n\t\t\t\t\t\t} else {\r\n\t\t\t\t\t\t\tthis.$message.error(res.data.msg);\r\n\t\t\t\t\t\t}\r\n\t\t\t\t\t});\r\n\t\t\t\t}\r\n\t\t\t});\r\n\t\t},\r\n\t\tresetForm(formName) {\r\n\t\t\tthis.$refs[formName].resetFields();\r\n\t\t}\r\n\t}\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.container {\r\n\tposition: relative;\r\n\r\n\t.form-item {\r\n\t\twidth: 80%;\r\n\t\tmargin: 0 auto 20px;\r\n\t}\r\n\r\n\t.rgs-form ::v-deep .el-input__inner,\r\n\t.rgs-form ::v-deep .el-select .el-input__inner {\r\n\t\tborder-radius: 4px;\r\n\t\tborder: 1px solid #ab47bc;\r\n\t\tbackground: #fff;\r\n\t\tcolor: #333;\r\n\t\theight: 40px;\r\n\t}\r\n\r\n\t.rgs-form ::v-deep .el-upload--picture-card,\r\n\t.rgs-form ::v-deep .el-upload .upload-img,\r\n\t.rgs-form ::v-deep .el-upload-list .el-upload-list__item,\r\n\t.rgs-form ::v-deep .el-upload .el-icon-plus {\r\n\t\tborder: 2px dashed #ab47bc;\r\n\t\tcolor: #7e57c2;\r\n\t\tbackground: #fff;\r\n\t\twidth: 160px;\r\n\t\theight: 80px;\r\n\t\tline-height: 80px;\r\n\t\tfont-size: 28px;\r\n\t\ttext-align: center;\r\n\t\tcursor: pointer;\r\n\t}\r\n}\r\n</style>\r\n"]}]}